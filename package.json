{
  "name": "vite-plugin-vux",
  "version": "1.0.0",
  "license": "MIT",
  "author": "JasonZhao",
  "description": "基于 Vite 插件，用于在 Vite 项目中使用 Vux 组件库",
  "keywords": [
    "vite",
    "plugin",
    "vux"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "NODE_ENV=development rollup --config rollup.config.ts --configPlugin typescript --watch",
    "build": "NODE_ENV=production rollup --config rollup.config.ts --configPlugin typescript",
    "release": "pnpm run build && npm publish"
  },
  "peerDependencies": {
    "vue": "^2.7.0-0",
    "vux": "^2.9.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "git-checks": false
  },
  "dependencies": {
    "@babel/core": "^7.26.9",
    "@babel/generator": "^7.26.10",
    "@babel/parser": "^7.26.10",
    "@babel/traverse": "^7.26.10",
    "babel-plugin-transform-commonjs": "^1.1.6",
    "babel-types": "^6.26.0",
    "js-yaml": "^4.1.0",
    "less": "^2.7.2",
    "lodash": "^4.17.21",
    "oxc-transform": "^0.58.1",
    "strip-comments": "^2.0.1",
    "strip-css-comments": "^3.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/node": "^22.13.4",
    "rollup": "^4.35.0",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "vite": "^6.1.1",
    "vue": "^2.7.0-beta.8"
  }
}